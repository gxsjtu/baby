<view class="container">
  <view class="view-container">
    <view wx:for="{{terms}}" wx:for-item="term" class="outer-s">
      <view class="name-s">
        {{term.name}}
      </view>
      <view style="display:flex;flex-direction:row;flex-wrap:wrap;">
        <view wx:for="{{term.docs}}" wx:for-item="item" class="{{item.isHas ? 'font-s' : 'no-s'}}">
          {{item.name}}
        </view>
      </view>
    </view>
  </view>
</view>